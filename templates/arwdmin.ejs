import { Link, routes } from '@redwoodjs/router'

import { useAuth } from 'src/auth'

const ArwdminArwdminPage = () => {
  const { isAuthenticated } = useAuth()

  return (
    <>
      <h1>aRWdmin</h1>
      {!isAuthenticated && (
        <>
          <Link to={routes.arwdminLogin()}>Log in</Link>
          <Link to={routes.arwdminSignup()}>Sign up</Link>
        </>
      )}
    </>
  )
}

export default ArwdminArwdminPage
